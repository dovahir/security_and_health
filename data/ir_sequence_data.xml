<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <record id="ir_sequence_security_situation" model="ir.sequence">
       <field name="name">Situation Reference</field>
       <field name="code">security.situation</field>
       <field name="prefix">SS</field>
       <field name="suffix">/%(month)s/%(year)s</field>
       <field name="padding">4</field>
       <field name="number_next">1</field>
       <field name="number_increment">1</field>
       <field name="company_id" eval="False"/>
       <field name="use_date_range" eval="True"/>
   </record>
</odoo>
